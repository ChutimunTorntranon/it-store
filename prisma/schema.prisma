generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {


  id String @id @default(cuid())
  name String 
  descrition String?
  price Float
  image String?
  category String
  brand String
  stock Int @ default(0)
  socket String? 
  wattage Int?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}